package arc.msoe.hmi.comms;
/**
 * A thread that reads from the Xbee wireless communicator and updates RTComms
 * 
 * 3/12/2015
 * @author beaverl
 *
 */
public class ReadThread extends Thread {

	volatile boolean running;
	
	public void start() {
		
	}
	
	
	public void run() {
		
		while (running) {
			System.out.println("collecting data...");
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
