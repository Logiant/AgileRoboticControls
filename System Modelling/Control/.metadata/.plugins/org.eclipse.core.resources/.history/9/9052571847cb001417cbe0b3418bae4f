package test;

public class HelloJNI {
	public HelloJNI() {
		String path = System.getProperty("user.dir") + "\\res\\";
//		System.out.println(path);
		System.load(path + "kernel32.dll");
		System.out.println("Load successful!");
		sleep(500);
	}
	
	private native void beep(int FREQUENCY, int DURATION);
	private native void sleep(int DURATION);
	
	
	public static void main(String[] args) {
		HelloJNI h = new HelloJNI();
		
		h.beep(698, 500);
	}
}
