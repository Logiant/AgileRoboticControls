package arc.msoe.hmi.comms;
/**
 * A thread that writes data from RTComms to the Xbee controller
 * 
 * 3/12/2015
 * @author beaverl
 *
 */
public class WriteThread extends Thread {

	RTComms comms;
	
	volatile boolean running = false;
	
	public WriteThread(RTComms comms) {
		this.comms = comms;
	}
}
